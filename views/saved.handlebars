        <div class="col s12 m12">
            <div class="card">
                <nav>
                    <div class="nav-wrapper indigo darken-4 white-text">
                        <span class="card-title" href={{{this.link}}}>{{{this.title}}}</span>
                        <a class="waves-effect waves-light btn amber">Remove From Saved</a>
                        <a class="waves-effect waves-light btn amber">Article Notes</a>
                    </div>
                </nav>
                <div class="card-content">
                    <p>{{{this.byline}}}</p>
                </div>
            </div>
        </div>
    </div>
    {{{/each}}}
