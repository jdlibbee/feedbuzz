    <nav>
        <div class="nav-wrapper indigo darken-4 white-text">
            <a href="#" class="brand-logo right">FeedBuzz!</a>
            <ul id="nave-mobile" class="left hide-on-med-and-down">
                <li><a href="">Home</a></li>
                <li><a href="">Saved Articles</a></li>
                <li><a class="waves-effect waves-light btn amber"><i class="material-icons left">get_app</i>Get New Articles!</a></li>
                <li><a class="waves-effect waves-light btn amber"><i class="material-icons left">leak_remove</i>Clear Articles!</a></li>
            </ul>
        </div>
    </nav>
</div>
<div class="parallax-container">
    <div class="section no-pad-bot">
        <div class="container">
            <br>
            <br>
            <img src="images/buzz.png" alt="intro" id="intro">
            <br>
            <br>
        </div>
    </div>
